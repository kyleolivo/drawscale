{
  "buildCommand": "npm run build",
  "outputDirectory": "dist",
  "redirects": [
    {
      "source": "/(.*)",
      "destination": "https://www.drawscale.app/$1",
      "permanent": true,
      "has": [
        {
          "type": "host",
          "value": "drawscale.app"
        }
      ]
    }
  ],
  "github": {
    "autoJobCancelation": true
  },
  "git": {
    "deploymentEnabled": {
      "main": false
    }
  }
}